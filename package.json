{
  "name": "wotwot-v2",
  "version": "1.0.0",
  "description": "This is the final restart",
  "main": "server/server.js",
  "scripts": {
    "dev-server": "webpack-dev-server",
    "serve": "webpack-dev-server --hot --inline && sass --watch client/css:client/css",
    "build": "webpack && npm run transpile",
    "start": "node server/lib/server",
    "server": "node-inspector --web-port 5858 & npm run transpile && node-dev --debug server/lib/server",
    "test": "ava --watch",
    "transpile": "babel server --ignore server/lib,server/static --out-dir server/lib"
  },
  "ava": {
    "failfast": true,
    "tap": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "author": "Devon McGrath",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.15.2",
    "css-loader": "^0.25.0",
    "enzyme": "^2.3.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "node-dev": "^3.1.3",
    "node-inspector": "^0.12.8",
    "node-sass": "^3.9.3",
    "react-addons-test-utils": "^15.2.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.15.2",
    "copy-webpack-plugin": "^3.0.1",
    "express": "^4.14.0",
    "firebase": "^3.4.0",
    "firebase-tools": "^3.0.7",
    "ramda": "^0.22.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "superagent": "^2.0.0",
    "webpack": "^1.13.1"
  }
}
